<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <RootNamespace>Coffee_to_go</RootNamespace>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Authentication\buttonImg\AccEnter.png" />
    <None Remove="Authentication\buttonImg\EmailLogo.png" />
    <None Remove="Authentication\buttonImg\GoogleLogo.png" />
    <None Remove="CommonStyle\controlImg\close.png" />
    <None Remove="CommonStyle\controlImg\closeHover.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Authentication\buttonImg\AccEnter.png" />
    <Resource Include="Authentication\buttonImg\EmailLogo.png" />
    <Resource Include="CommonStyle\controlImg\closeHover.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="CommonStyle\controlImg\close.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="FirebaseAuthentication.WPF" Version="4.0.2" />
    <PackageReference Include="MahApps.Metro" Version="2.4.9" />
    <PackageReference Include="Nancy" Version="2.0.0" />
    <PackageReference Include="PuppeteerSharp" Version="10.0.0" />
    <PackageReference Include="Selenium.WebDriver" Version="4.10.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Authentication\current_user\" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Authentication\buttonImg\GoogleLogo.png" />
  </ItemGroup>

</Project>
